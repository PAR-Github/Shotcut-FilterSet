<mlt LC_NUMERIC="C" version="7.17.0" root="" parent="producer0" in="00:00:00.000" out="00:10:25.609">
	<producer id="producer0" in="00:00:00.000" out="00:10:25.609">
		<property name="length">15000</property>
		<property name="eof">pause</property>
		<property name="resource">black</property>
		<property name="aspect_ratio">1</property>
		<property name="mlt_service">color</property>
		<property name="shotcut:filtersClipboard">1</property>
		<filter id="filter0" in="00:00:22.356" out="00:00:22.606">
			<property name="version">2.6</property>
			<property name="mlt_service">frei0r.bigsh0t_eq_to_rect</property>
			<property name="shotcut:filter">bigsh0t_eq_to_rect</property>
			<property name="yaw">0</property>
			<property name="pitch">0</property>
			<property name="roll">0</property>
			<property name="fov">0=148;5=34</property>
			<property name="fisheye">0</property>
			<property name="interpolation">1</property>
		</filter>
		<filter id="filter1" in="00:00:22.356" out="00:00:22.606">
			<property name="version">1.1</property>
			<property name="mlt_service">frei0r.rgbsplit0r</property>
			<property name="0">0.4</property>
			<property name="1">0=0.5;5=0.471</property>
		</filter>
		<filter id="filter2" in="00:00:22.356" out="00:00:22.606">
			<property name="version">2.6</property>
			<property name="mlt_service">frei0r.bigsh0t_eq_to_rect</property>
			<property name="shotcut:filter">bigsh0t_eq_to_rect</property>
			<property name="yaw">0</property>
			<property name="pitch">0</property>
			<property name="roll">0</property>
			<property name="fov">148</property>
			<property name="fisheye">0</property>
			<property name="interpolation">1</property>
			<property name="disable">0</property>
		</filter>
		<filter id="filter3" in="00:00:22.356" out="00:00:22.564">
			<property name="mlt_service">avfilter.gblur</property>
